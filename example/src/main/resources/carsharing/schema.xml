        <dimension name="distance"/>
        <dimension name="doors"/>
        <dimension name="gps_lat"/>
        <dimension name="gps_long"/>
        <dimension name="make"/>
        <dimension name="make_desc"/>
        <dimension name="max_range"/>
        <dimension name="model"/>
        <dimension name="plate_number"/>
